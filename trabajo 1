class vehiculo:

    def __init__ (self,marca, modelo, year):

        self.marca= marca

        self.modelo=modelo

        self.year=year

        

    def arrancar(self):

        return f"El {self.marca} { self.modelo} esta arranancando"

    

    def detener(self):

        return f"El {self.marca} { self.modelo} esta detenido."

        

    #subclase

class moto(vehiculo):

    def __init__(self,marca, modelo,year, cilindraje):

        super().__init__(marca,modelo,year)

        self.cilindraje=cilindraje

    def tiene_caballos(self):

        return f"El {self.marca} {self.modelo} si tiene caballos ."

        

def probar_vehiculo(vehiculo):

     print(vehiculo.arrancar)

     print(vehiculo.detener)

    

MoTO1 = moto("yamaha","MT-07",2022,689)

MoTO2 = moto("honda","cb190",2022,190)

    

probar_vehiculo(moto)

probar_vehiculo(moto)

print(MoTO1.tiene_caballos())

print(MoTO2.detener())
